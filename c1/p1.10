(define (f n) (A 0 n))
(f n)
= (A 0 n)
= 2 * n

(define (g n) (A 1 n))
(g n)
= (A 1 n)
= (A 0 (A 1 n-1))
= (A 0 (A 0 (A 1 n-2)))
= ...
= (A 0 (A 0 (A 0 ... (A 1 1))))
= 2 * 2 * 2 * ... * 2
= 2^n


(define (h n) (A 2 n))
(h n)
= (A 1 (A 2 n-1))
= (A 1 (A 1 (A 2 n-2)))
= ...
= (A 1 (A 1 .... (A 2 1)))
= (A 1 (A 1 ... (A 1 2)))
= (g (g ...(g 2)))
= (g (g ...2^2))
= 2^(2^(2^2))...




 